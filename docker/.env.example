# NEON Docker Configuration
# Copy this file to .env and update the values
# Or run the setup script: ./scripts/setup.sh

# Domains
DOMAIN=neon.example.com
LIVEKIT_DOMAIN=livekit.example.com

# PostgreSQL
POSTGRES_USER=neon
POSTGRES_PASSWORD=change-this-password
POSTGRES_DB=neon

# Redis
REDIS_PASSWORD=change-this-password

# LiveKit (generate key: echo "API$(openssl rand -hex 12 | tr '[:lower:]' '[:upper:]')")
LIVEKIT_API_KEY=APIxxxxxxxx
LIVEKIT_API_SECRET=your-livekit-secret

# Garage S3
GARAGE_ADMIN_TOKEN=your-garage-admin-token
S3_ACCESS_KEY=NEONxxxxxxxx
S3_SECRET_KEY=your-s3-secret-key
S3_BUCKET=neon-storage

# Admin
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=change-this-password
